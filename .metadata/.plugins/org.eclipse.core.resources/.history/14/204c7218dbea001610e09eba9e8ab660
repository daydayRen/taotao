package it.base;

import java.util.Scanner;

public class Factorial {

	public static void main(String[] args) {
		@SuppressWarnings("resource")
		Scanner in=new Scanner(System.in);
		int result;
		int temp=1;
		int n=in.nextInt();
		for(int i=1;i<=n;i++){
			 temp*=i;
			 
		}
		result=temp;
		System.out.println(result);

	}

}
