package it.base;

import java.util.Scanner;

public class Test1 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String s1=sc.next();
		//String s2=sc.next();
		int[] A=new int[100];
		int[] B=new int[100];
		for(int i=s1.length()-1;i>=0;i--){
			A[i]=s1.charAt(i)-'0';
			System.out.println(A[i]);
		}
		boolean begin=false;
		for(int i=A.length-1;i>=0;i--){
			if(begin){
				System.out.print(A[i]);
				continue;
			}
			if(A[i-1]!=0){
				begin=true;
				
			}
		}
		
	}

}
