package it.base;

import java.util.Scanner;

public class XinPin {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int a[][]=new int[n][n];
		int i,j;
		int count[]=new int[n];
		for(i=0;i<n;i++){
			count[i]=1;
			for(j=0;j<n;j++){
				a[i][j]=sc.nextInt();
			}
		}
		
		for(i=0;i<n;i++)
		{
			for(j=0;j<n;j++){
				if(i!=j&&a[j][i]==1){
					count[i]++;
				}
				
				if(count[i]>(n/2)&&i<(n-1)){
					System.out.print((i+1)+" ");
				}
					
			}
		}
		
	}

}
