package cyclic;

public class CubeNum {
	@SuppressWarnings("null")
	public static int cute(int i){
		int[] j = null;
		int t=0;
		int sum = 0;
		if(i>0){
			j[t++]=i%10;
			i/=10;
		}
		for(;t>0;t--){
			sum+=Math.pow(j[t], 3);
		}
		return sum;
		
	}
	public static void main(String[] args) {
		int sum=0;
		System.out.println(cute(100));
		for(int i=100;i<1000;i++){
			
		}
	}

}
